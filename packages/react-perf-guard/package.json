{
  "name": "react-perf-guard",
  "version": "0.1.2",
  "description": "Dev-only performance guardrail for React and Next.js using React Profiler",
  "author": "Amiya Das",
  "repository": {
    "type": "git",
    "url": "https://github.com/amiyaDev/react-perf-guard"
  },
  "homepage": "https://github.com/amiyaDev/react-perf-guard#readme",
  "bugs": {
    "url": "https://github.com/amiyaDev/react-perf-guard/issues"
  },
  "license": "MIT",
  "private": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "prepublishOnly": "npm run clean && npm run build",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "react",
    "performance",
    "react-profiler",
    "devtools",
    "nextjs",
    "profiling",
    "performance-monitoring"
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "typescript": "~5.9.3"
  }
}
